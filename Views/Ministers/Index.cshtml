@model IList<ministers_of_sweden.web.ViewModels.IndexViewModel>

<section class="page">
    <h1 class="page-title">Aktuella bilar för försäljning</h1>

    <div class="gallery-wrapper">
        @foreach (var minister in Model)
        {
            <div class="gallery-card">
                <div>@minister.Name</div>
                <img src="@minister.ImageUrl" alt="@minister.Born">
                
                </div>
        }